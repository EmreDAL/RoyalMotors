{
  "name": "gulp4-webpack4",
  "version": "1.0.0",
  "description": "Modern Front-end Boilerplate",
  "scripts": {
    "dev": "gulp --require babel-register --gulpfile tasks",
    "build": "cross-env NODE_ENV=production gulp build --require babel-register --gulpfile tasks",
    "build:js": "cross-env NODE_ENV=production gulp scripts --require babel-register --gulpfile tasks",
    "build:css": "cross-env NODE_ENV=production gulp css --require babel-register --gulpfile tasks",
    "build:img": "cross-env NODE_ENV=production gulp images --require babel-register --gulpfile tasks",
    "build:html": "cross-env NODE_ENV=production gulp html --require babel-register --gulpfile tasks"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "babelrc": false
  },
  "author": {
    "name": "Enes Sefa Küccük",
    "email": "enes.sefa.k@gmail.com",
    "url": "https://github.com/enessefak/Gulp-4-with-Webpack-4"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.4",
    "critical": "^1.3.2",
    "cross-env": "^5.1.6",
    "del": "^3.0.0",
    "flexiblegs-scss": "^5.6.0",
    "flexiblegs-scss-plus": "^5.6.0",
    "gulp": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.2.3",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svg-sprites": "^4.1.2",
    "node-sass": "^4.11.0",
    "sw-precache": "^5.2.1",
    "webpack": "^4.9.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "normalize-css": "^2.3.1",
    "slick-carousel": "^1.8.1"
  }
}
